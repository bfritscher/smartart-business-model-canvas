	<layoutNode>
		<alg type="snake">
			<param type="grDir" val="tL" />
			<param type="flowDir" val="row" />
			<param type="bkpt" val="fixed" />
			<!-- 7 days plus 7 spaces -->
			<param type="bkPtFixedVal" val="14" />
		</alg>
		<shape />
		<presOf />
		<constrLst>
			<constr type="w" for="ch" forName="day" refType="w" />
			<constr type="h" for="ch" forName="day" refType="w" refFor="ch" refForName="day" fact="0.9" />

			<constr type="w" for="ch" forName="negSp" refType="w" fact="-0.01" />
			<constr type="h" for="ch" forName="negSp" val="0" />

			<constr op="equ" type="primFontSz" for="des" forName="numberTx" val="65" />
			<constr op="equ" type="primFontSz" for="des" forName="eventsTx" val="65" />
		</constrLst>
		<ruleLst />

		<forEach axis="ch" ptType="node">
			<layoutNode name="day">
				<alg type="composite" />
				<shape />
				<presOf />
				<constrLst>
					<constr type="r" for="ch" forName="eventsSp" refType="w" />
					<constr type="b" for="ch" forName="eventsSp" refType="h" />
					<constr type="w" for="ch" forName="eventsSp" refType="w" fact="0.9" />
					<constr type="h" for="ch" forName="eventsSp" refType="h" fact="0.9" />

					<constr type="l" for="ch" forName="numberTx" val="0" />
					<constr type="t" for="ch" forName="numberTx" val="0" />
					<constr type="w" for="ch" forName="numberTx" refType="w" />
					<constr type="h" for="ch" forName="numberTx" refType="h" />
					<constr op="lte" type="h" for="ch" forName="numberTx" refType="w" refFor="ch" refForName="numberTx" />
					<constr op="lte" type="w" for="ch" forName="numberTx" refType="h" refFor="ch" refForName="numberTx" />
					<constr op="lte" type="w" for="ch" forName="numberTx" refType="w" fact="0.3" />
					<constr op="lte" type="h" for="ch" forName="numberTx" refType="h" fact="0.3" />

					<constr type="r" for="ch" forName="eventsTx" refType="w" />
					<constr type="b" for="ch" forName="eventsTx" refType="h" />
					<constr type="w" for="ch" forName="eventsTx" refType="w" fact="0.9" />
					<constr type="h" for="ch" forName="eventsTx" refType="h" fact="0.75" />
				</constrLst>
				
				<layoutNode name="eventsSp" styleLbl="bgAccFollowNode1">
					<varLst>
						<bulletEnabled val="true" />
					</varLst>
					<alg type="sp" />
					<shape type="rect" />
					<presOf axis="des" ptType="node" />
				</layoutNode>
				
				<layoutNode name="numberTx">
					<alg type="tx" />
					<shape type="ellipse" />
					<presOf axis="self" ptType="node" />
					<constrLst>
						<constr type="lMarg" val="0" />
						<constr type="rMarg" val="0" />
						<constr type="tMarg" val="0" />
						<constr type="bMarg" val="0" />
					</constrLst>
					<ruleLst>
						<rule type="primFontSz" val="5" />
					</ruleLst>
				</layoutNode>

				<layoutNode name="eventsTx" styleLbl="alignAccFollowNode1">
					<varLst>
						<bulletEnabled val="true" />
					</varLst>
					<alg type="tx">
						<param type="stBulletLvl" val="1" />
					</alg>
					<shape type="rect" hideGeom="true" />
					<presOf axis="des" ptType="node" />
					<constrLst>
						<constr type="secFontSz" refType="primFontSz" />
					</constrLst>
					<ruleLst>
						<rule type="primFontSz" val="5" />
					</ruleLst>
				</layoutNode>
			</layoutNode>

			<layoutNode name="negSp">
				<alg type="sp" />
				<shape />
				<presOf />
			</layoutNode>
		</forEach>
	</layoutNode>
</layoutDef>